{
  "name": "golden-hammer-ui",
  "version": "0.1.0",
  "description": "",
  "author": "nfgCodex <nfg.codex@outlook.com>",
  "license": "MIT",
  "source": "./src/index.html",
  "scripts": {
    "clean": "rimraf ./dist ./.parcel-cache",
    "docker-compose": "npm i --force && echo \"Running in docker-compose... You should run interactively by attaching:\n\t\tdocker exec -it `hostname` npm run dev\" && tail -f /dev/null",
    "build": "parcel build --no-source-maps",
    "start": "node ./app",
    "dev": "NODE_ENV=development concurrently -r 'npm:dev-concurrently*'",
    "dev-concurrently:mkdir": "mkdir -p dist",
    "dev-concurrently:watch": "parcel serve",
    "check": "tsc --noEmit"
  },
  "targets": {
    "default": {
      "distDir": "./dist",
      "publicUrl": "."
    }
  },
  "dependencies": {
    "@mantine/core": "^3.6.11",
    "@mantine/hooks": "^3.6.11",
    "@mantine/modals": "^3.6.11",
    "@mantine/notifications": "^3.6.11",
    "buffer": "^6.0.3",
    "react": "^17.0.2",
    "react-base-table": "^1.13.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "socket.io-client": "^4.4.1",
    "uuid": "^8.3.2",
    "zustand": "^3.7.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.3.2",
    "@parcel/validator-typescript": "^2.3.2",
    "@types/node": "^17.0.21",
    "@types/react-dom": "^17.0.11",
    "@types/react": "^17.0.39",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.0.0",
    "golden-hammer-shared": "bitbucket:nerdfoundrygaming/golden-hammer-shared",
    "nodemon": "^2.0.15",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "parcel": "^2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "alias": {
    "-": "./src/scripts"
  }
}
